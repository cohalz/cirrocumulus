{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "prettier": [
      true,
      {
        "jsxBracketSameLine": false,
        "semi": false,
        "trailingComma": "es5"
      }
    ],
    "interface-name": [
      true,
      "never-prefix"
    ]
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ]
}
